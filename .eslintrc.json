{
    "env": {
        "browser" : true,
        "node" : true,
        "es6" : true
    },
    "parserOptions": {
        "ecmaFeatures": {
          "jsx": true
        },
        "sourceType": "module"
    },
    "extends": ["eslint:recommended", "google"],
    "plugins": ["react"],
    "rules": {
      "no-unused-vars": 0,
      "require-jsdoc": 0,
      "object-curly-spacing": 0
    }
}